    "chart.js": "^3.9.1",
    "classnames": "^2.3.2",
    "jspdf": "^2.5.1",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
